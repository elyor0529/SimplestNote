<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.NoteVersionsEntity" table="note_versions" schema="" catalog="simplestnote">
        <id name="id">
            <column name="id" sql-type="bigint unsigned" not-null="true"/>
        </id>
        <property name="noteId">
            <column name="note_id" sql-type="bigint" not-null="true"/>
        </property>
        <property name="modifiedDate">
            <column name="modified_date" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>